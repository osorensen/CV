  langid = {english},
  file = {/Users/oystein/Zotero/storage/ALCE9BCS/Vidal-Pineiro et al. - 2020 - Cellular correlates of cortical thinning throughou.pdf;/Users/oystein/Zotero/storage/HX779HHY/s41598-020-78471-3.html}
