  author = {Walhovd, Kristine B. and Fjell, Anders M. and S{\o}rensen, {\O}ystein and Mowinckel, Athanasia Monika and Reinbold, C{\'e}line Sonja and Idland, Ane-Victoria and Watne, Leiv Otto and Franke, Andre and Dobricic, Valerija and Kilpert, Fabian and Bertram, Lars and Wang, Yunpeng},
  year = {2020},
